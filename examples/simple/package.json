{
  "name": "app",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "private",
  "scripts": {
    "init": "node ./database/init.js",
    "init:test": "node ./database/init.js -t",
    "console": "ts-node -r tsconfig-paths/register app/console.ts",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"app/**/*.ts\" \"artisan/**/*.ts\" \"common/**/*.ts\" \"config/**/*.ts\" \"plugin/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register app/app/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/app/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk=8026 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "migration:create": "npx ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:create -n ",
    "migration:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -n",
    "migration:run": "RUN_MODE=typeorm npx ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "RUN_MODE=typeorm npx ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert",
    "migration:run:test": "NODE_ENV=test RUN_MODE=typeorm npx ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.0",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nest-middlewares/serve-static": "^6.0.0",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.2.4",
    "@nestjs/core": "^6.2.4",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.2.4",
    "@nestjs/platform-socket.io": "^6.2.4",
    "@nestjs/typeorm": "^6.1.2",
    "@nestjs/websockets": "^6.2.4",
    "@nestjsx/crud": "^3.2.0",
    "@sigodenjs/wechatpay": "^2.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/lodash": "^4.14.133",
    "ansi-colors": "^3.2.4",
    "bcrypt": "^3.0.6",
    "chalk": "^2.4.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "commander": "^2.20.0",
    "create-output-stream": "0.0.1",
    "exceljs": "^1.12.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "ioredis": "^4.9.5",
    "lodash": "^4.17.11",
    "module-alias": "^2.2.0",
    "moment": "^2.24.0",
    "nest-schedule": "^0.4.6",
    "nest-winston": "^1.0.4",
    "nestin-common": "^0.1.4-alpha.0",
    "nestjs-config": "^1.4.0",
    "nestjs-console": "^1.0.2",
    "nestjs-log4js": "^0.1.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^7.11.0",
    "pm2": "^3.5.1",
    "qs": "^6.7.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2",
    "shelljs": "^0.8.3",
    "typeorm": "^0.2.17",
    "typeorm-transactional-cls-hooked": "^0.1.8",
    "typescript": "^3.5.1",
    "wechat-oauth": "^1.5.0",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/testing": "^6.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/ioredis": "^4.0.11",
    "@types/jest": "^23.3.1",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.14.8",
    "@types/passport": "^1.0.0",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-local": "^1.0.33",
    "@types/qs": "^6.5.3",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.5",
    "@types/xml2js": "^0.4.4",
    "jest": "^23.5.0",
    "jest-cucumber": "^2.0.11",
    "lerna": "^3.16.4",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "supertest": "^3.1.0",
    "swagger-ui-express": "^4.0.5",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/../app$1.ts",
      "^@common(.*)$": "<rootDir>/../common$1.ts",
      "^@plugin(.*)$": "<rootDir>/../plugin$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "app/",
    "testMatch": [
      "**/*.spec.ts",
      "**/*.steps.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "standard": {
    "ignore": [
      "dist/**/*.js"
    ]
  }
}
